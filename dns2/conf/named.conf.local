//
// Do any local configuration here
zone "starwars.lan" {
	type slave;
        file "/etc/bind/db.starwars.lan";
        allow-transfer { 192.168.20.11; };
};        
      
zone "20.168.192.in-addr.arpa" {
	type slave;
    file "/etc/bind/db.192.168.20";
    masters { 192.168.0.10; };
};
